{:source-paths
 ["src"]

 :dependencies
 [[reagent "1.0.0"]
  [binaryage/devtools "1.0.3"]]

 :builds
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"

        :modules {:main {:init-fn lopen.core/init}}

        :devtools {:after-load lopen.core/init
                   :http-root "public"
                   :http-port 3000}}}}
